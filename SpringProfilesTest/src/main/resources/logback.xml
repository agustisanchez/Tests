<?xml version="1.0"?>
<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss} [%X{contextPath}]%-5level %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>
	<logger name="profilespocclient.ServiceClient" level="INFO" />

	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
